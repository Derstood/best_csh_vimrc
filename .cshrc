set prompt="%W-%D %P @%n %//"

alias vc 'gvim ~/.cshrc'
alias sc 'source ~/.cshrc'
    
alias mrm 'module remove'
alias mlist 'module list'
alias mlo 'module load'
alias mav 'module ava'

alias g 'gvim -pO'
alias gd 'gvimdiff'
alias his 'history'
alias mdir 'mkdir -p'
alias l 'ls -alrt'
alias lld 'ls -alrt -d */'
alias cp 'cp -rp'
alias cd 'cd \!* ; l'
alias f 'find . -name '
    
alias . 'cd ..'
alias .. 'cd ../..'
alias . 'cd ../../..'
    
alias mydu 'source ~/mydu.csh'
